[hwpack]
format=2.0
name=linaro-leb-origen
architectures=armel
origin=Linaro
maintainer=Linaro Platform <linaro-dev@lists.linaro.org>
support=supported
packages=linux-image-linaro-lt-origen ubuntu-origen-extras
assume-installed=linaro-headless
mmc_id=0:2
